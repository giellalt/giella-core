<!ELEMENT r (e+) >
<!ELEMENT e (lg,mg+) >
<!ATTLIST e 
      usage ( mt | dic | ped | other ) #IMPLIED
      src ( nj | other ) #IMPLIED
>      
<!ELEMENT lg (l,lc*) >
<!ELEMENT l (#PCDATA) >
<!ATTLIST l 
      pos ( a | n | v | adv | po | pr | pron | rel | pers | i | pcle | num | cc | cs | n | prop | x ) #REQUIRED
      nr ( sg | pl ) #IMPLIED
      type ( rel | pers | pre | post ) #IMPLIED
>      


<!ELEMENT lc (#PCDATA) >

<!ELEMENT mg (tg+) >
<!ELEMENT tg (re?,t+,xg*) >

<!ELEMENT re (#PCDATA) >
<!ELEMENT t (#PCDATA) >
<!ATTLIST t 
      pos ( a | a5 | S | m | f | n | v | adv | p | pron | prop | i | cc | cs | im | mpl | fpl | npl | num | pcle | s | x ) #REQUIRED
      nr ( sg | pl ) #IMPLIED
>      

<!ELEMENT xg ((x,xt?)+)>
<!ELEMENT x (#PCDATA)>
<!ELEMENT xt (#PCDATA)>


