<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty">
   <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
<xs:include schemaLocation="semanticSetFor-dict-schema.xsd"/>
<xs:include schemaLocation="kpv-dict-schema.xsd"/>
<xs:include schemaLocation="mrj-dict-schema.xsd"/>
<xs:include schemaLocation="myv-dict-schema.xsd"/>
   <xs:element name="r">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="e"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="e">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="lg"/>
            <xs:element ref="pos"/>
            <xs:element minOccurs="0" ref="semRole"/>
            <xs:element minOccurs="0" ref="phonet"/>
            <xs:element minOccurs="0" ref="morphg"/>
	   <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="drv"/>
            <xs:element ref="etymg"/>
	   </xs:choice>
            <xs:element maxOccurs="unbounded" ref="mg"/>
         </xs:sequence>
         <xs:attribute name="exclude">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="fst"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="order">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="1"/>
                  <xs:enumeration value="2"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="lg">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="l"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="lsub"/>
            <xs:element ref="stg"/>
            <xs:element ref="stress" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="stg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="st"/>
            <xs:element ref="stCtn"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="l">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="pos"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
<xs:element name="semRole" type="xs:string"/>
   <xs:element name="lsub">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="myvSrc"/>
               <xs:attribute ref="orthType"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="st">
      <xs:complexType mixed="true">
         <xs:sequence/>
         <xs:attribute ref="Contlex"/>
         <xs:attribute ref="orthType"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="stCtn">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="2" ref="st"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="src"/>
         </xs:sequence>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="hyphenated"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="stress">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="pos" type="posType"/>
   <xs:attribute name="pos" type="posType"/>
   <xs:simpleType name="posType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="SUF"/>
         <xs:enumeration value="PREF"/>
         <xs:enumeration value="PHRASE_A"/>
         <xs:enumeration value="PHRASE_ADV"/>
         <xs:enumeration value="PHRASE_N"/>
         <xs:enumeration value="PHRASE_NUM"/>
         <xs:enumeration value="PHRASE_PRON"/>
         <xs:enumeration value="PHRASE_QNT"/>
         <xs:enumeration value="PHRASE_SPAT"/>
         <xs:enumeration value="PHRASE_TEMP"/>
         <xs:enumeration value="PHRASE_V"/>
	 <xs:enumeration value="BAHUVRIHI">
<!-- This is an NP that functions as either a noun modifier in attribute position or as a complement without a direct semantic relation, e.g. "red head" indicates "a red-headed person" not merely a head that is red.  -->	 </xs:enumeration>
         <xs:enumeration value="A"/>
         <xs:enumeration value="AD-A"/>
         <xs:enumeration value="ATTR">
<!-- adjective that only occurs in attribute position, e.g. fin:"eri" -->	 </xs:enumeration>
         <xs:enumeration value="ADDITIVE">
<!-- too, also -->	 </xs:enumeration>
         <xs:enumeration value="PRED_A"/>
         <xs:enumeration value="PRED_IDEOPH">
<!-- IDEOPH used as a predication в роли сказ. -->
	 </xs:enumeration>
         <xs:enumeration value="PRED_N">
<!-- в роли сказ. -->	 </xs:enumeration>
         <xs:enumeration value="ADN_A"/>
         <xs:enumeration value="ADN_A-INDEF"/>
         <xs:enumeration value="A-1ARG"/>
         <xs:enumeration value="A-INDEF"/>
         <xs:enumeration value="A-SUPERL"/>
         <xs:enumeration value="V-GEN_A"/>
         <xs:enumeration value="COMP_A"/>
         <xs:enumeration value="ADV"/>
         <xs:enumeration value="ADV-MANNER"/>
         <xs:enumeration value="ADV-SPAT"/>
         <xs:enumeration value="ADV-TEMP"/>
<!-- "CEK" indicates the form of the adverb "Erzya" -->
         <xs:enumeration value="CEK_ADV"/>
<!-- "NEK" indicates the form of the adverb "Erzya" -->
         <xs:enumeration value="NEK_ADV"/>
         <xs:enumeration value="PRON_ADV"/>
         <xs:enumeration value="ADV-GEN"/>
         <xs:enumeration value="ADV-TRA"/>
         <xs:enumeration value="ADV-IDEOPH"/>
         <!--вырк, ноцк, штёлк-->
         <xs:enumeration value="ADV-IDEOPH-INE">
            <!--баксорсо-->
         </xs:enumeration>
         <xs:enumeration value="ADV-MANNER-INDEF"/>
         <xs:enumeration value="ADV-PARENTHETIC"/>
         <xs:enumeration value="FINITE_PARENTHETIC"/>
         <xs:enumeration value="PCLE_ADV-PARENTHETIC"/>
         <xs:enumeration value="PCLE_EPIST">
            <!--particle Epistemological modality-->
         </xs:enumeration>
         <xs:enumeration value="V_ADV-PARENTHETIC"/>
         <xs:enumeration value="ADV-POSITION"/>
         <xs:enumeration value="ADV-STATE"/>
         <xs:enumeration value="PRED2">
            <!--secondary predication: чавонь кедть, ласте, озадо-->
         </xs:enumeration>
         <xs:enumeration value="TEMP-0ARG-INDEF"/>
         <xs:enumeration value="TEMP-INDEF"/>
         <xs:enumeration value="TEMP-DEICT"/>
         <xs:enumeration value="ART"/>
         <xs:enumeration value="ASSOC-COLL"/>
         <xs:enumeration value="CLT"/>
         <xs:enumeration value="A-PRED">
            <!--A-PRED >> ??COMPL "complement Russian grammars tend to use the term 'наречия' with Full statements _Холодно!_. "-->
         </xs:enumeration>
         <xs:enumeration value="CONJ">
            <!--Conjunction that still needs to classified as either coordinator or subordinator-->
         </xs:enumeration>
         <xs:enumeration value="CC">
            <!--COORD "coordinator"-->
         </xs:enumeration>
         <xs:enumeration value="CC-ADDITIVE">
<!--соед. -->	 </xs:enumeration>
         <xs:enumeration value="CC-DISJUNCTIVE">
<!-- раздел. -->	 </xs:enumeration>
         <xs:enumeration value="CC-ADVERSATIVE"/>
         <xs:enumeration value="CC-CORRELATIVE">
<!-- присоединит.-->
	 </xs:enumeration>
         <xs:enumeration value="CC-EMPHATIC">
<!--усил., выделит. -->
	 </xs:enumeration>
         <xs:enumeration value="CS">
            <!--SUBORD "subordinator"-->
         </xs:enumeration>
         <xs:enumeration value="CS-CAUSE">
<!-- причин. потому что -->
	 </xs:enumeration>
         <xs:enumeration value="CS-COMPARISON">
<!-- сравнит. -->
	 </xs:enumeration>
         <xs:enumeration value="CS-CONCESSIVE">
<!--уступ. -->
	 </xs:enumeration>
         <xs:enumeration value="CS-CORRELATIVE">
<!-- кымын ... сымын : чем ... тем -->
	 </xs:enumeration>
         <xs:enumeration value="CS-CORRELATIVE-MAIN">
<!-- кымын ... сымын : чем ... тем Main clause marker-->
	 </xs:enumeration>
         <xs:enumeration value="DAT-1ARG"/>
         <xs:enumeration value="DEM"/>
         <xs:enumeration value="DEM-ADN"/>
         <xs:enumeration value="DEM-ADV"/>
         <xs:enumeration value="PRON-DEM"/>
         <xs:enumeration value="DET"/>
         <xs:enumeration value="DET-PRON"/>
         <xs:enumeration value="DET-ADN"/>
         <xs:enumeration value="DET-PL"/>
         <xs:enumeration value="FORMULAIC"/>
         <xs:enumeration value="GEN-ATTR"/>
         <xs:enumeration value="ABE_A"/>
         <xs:enumeration value="CAR_A"/>
         <xs:enumeration value="CAR_ADV"/>
         <xs:enumeration value="GEN_A"/>
         <xs:enumeration value="EXCLUSIVE">
            <!--Exclusive: just, only, alone-->
         </xs:enumeration>
         <xs:enumeration value="GER"/>
         <xs:enumeration value="GER-ZJ"/>
         <xs:enumeration value="GER-ZJ_A"/>
         <xs:enumeration value="GER-ZJ_MANNER"/>
         <xs:enumeration value="GER-ZJ_N"/>
         <xs:enumeration value="GER-ZJ_STATE"/>
         <xs:enumeration value="IDEOPH"/>
         <xs:enumeration value="IDEOPH-INE"/>
         <xs:enumeration value="IMITATIVE"/>
         <xs:enumeration value="INTERJ"/>
         <xs:enumeration value="INTERJ-IMP"/>
         <xs:enumeration value="INTERJ-CONATIVE"/>
         <xs:enumeration value="INSTR">
            <!--Instrument-->
         </xs:enumeration>
         <xs:enumeration value="MODIFIER"/>
         <xs:enumeration value="DEP-MORPH"/>
         <xs:enumeration value="DV_N"/>
         <xs:enumeration value="N"/>
         <xs:enumeration value="N.ABE"/>
         <xs:enumeration value="N.COMP"/>
         <xs:enumeration value="N.GEN"/>
         <xs:enumeration value="INE_N"/>
         <xs:enumeration value="NN"/>
         <xs:enumeration value="N-ASSOC"/>
         <xs:enumeration value="N-COLL"/>
         <xs:enumeration value="N-PL"/>
         <xs:enumeration value="N-SG"/>
         <xs:enumeration value="N-TEMP">
            <!--Temporal noun: The nominative singular is the same as the Temporal adverb form-->
         </xs:enumeration>
         <xs:enumeration value="N-VOCATIVE">
<!-- depricating to VOCATIVE -->
	 </xs:enumeration>
         <xs:enumeration value="VOCATIVE"/>
         <xs:enumeration value="NUM"/>
         <xs:enumeration value="NUM-ADN"/>
         <xs:enumeration value="NUM-UNIV"/>
         <xs:enumeration value="NUM-DISTR">
<!-- кыкӧн-кыкӧн -->
	 </xs:enumeration>
         <xs:enumeration value="CARD"/>
         <xs:enumeration value="CARD-ADV"/>
         <xs:enumeration value="CARD-APPR"/>
         <xs:enumeration value="CARD-ITER"/>
         <xs:enumeration value="CARD-ITER-APPR">
<!-- кыкысь-куимысь -->
	 </xs:enumeration>
         <xs:enumeration value="ADV-ITER"/>
         <xs:enumeration value="ITER"/>
         <xs:enumeration value="CARD-PL"/>
         <xs:enumeration value="NUM-ORD"/>
         <xs:enumeration value="ORD"/>
         <xs:enumeration value="ORD-ITER"/>
         <xs:enumeration value="UNIV-ITER"/>
         <xs:enumeration value="PCLE_AFFIRM">
<!-- утверд. (да) -->
	 </xs:enumeration>
         <xs:enumeration value="PCLE_CONCESSIVE">
<!-- уступ. (хотя)-->
	 </xs:enumeration>
         <xs:enumeration value="PCLE-DEICT"/>
         <xs:enumeration value="PCLE-DEM"/>
         <xs:enumeration value="PCLE-DESCR"/>
         <xs:enumeration value="PCLE-IMP"/>
         <xs:enumeration value="PCLE"/>
         <xs:enumeration value="PCLE_NEG"/>
         <xs:enumeration value="PCLE_OPT"/>
         <xs:enumeration value="PCLE-ONOM"/>
         <xs:enumeration value="PCLE_COMPAR-STND">
            <!--Comparative standard-->
         </xs:enumeration>
         <xs:enumeration value="PCLE_STRENGTH">
<!-- усилит. strengthening -->
	 </xs:enumeration>
         <xs:enumeration value="PCLE_SUPERL"/>
         <xs:enumeration value="ADV_SUPERL"/>
         <xs:enumeration value="POSTP"/>
         <xs:enumeration value="POSTP-SPAT"/>
         <xs:enumeration value="POSTP-TEMP"/>
         <xs:enumeration value="PREP"/>
         <xs:enumeration value="PRON"/>
         <xs:enumeration value="PRON-NEG"/>
         <xs:enumeration value="PRON-EXCL"/>
         <xs:enumeration value="PRON-GEN"/>
         <xs:enumeration value="PRON-INDEF"/>
         <xs:enumeration value="PRON-NONSPEC"/>
         <xs:enumeration value="PRON-PERS"/>
         <xs:enumeration value="PRON-PERS-INTENSIVE"/>
         <xs:enumeration value="PERS-DAT"/>
         <xs:enumeration value="PERS-INDEF"/>
         <xs:enumeration value="PRON-RECIP"/>
         <xs:enumeration value="PRON-REFL"/>
         <xs:enumeration value="ADN-REFL"/>
         <xs:enumeration value="PRON-REL"/>
         <xs:enumeration value="PROP"/>
         <xs:enumeration value="PROP-ANIM"/>
         <xs:enumeration value="PROP-DEITY"/>
         <xs:enumeration value="PROP-FirstM"/>
         <xs:enumeration value="PROP-HUM"/>
         <xs:enumeration value="PROP-INANIM"/>
         <xs:enumeration value="PROP-PL"/>
         <xs:enumeration value="PRC"/>
         <xs:enumeration value="PTC"/>
         <xs:enumeration value="PTC-I_A"/>
         <xs:enumeration value="PTC-CJA"/>
         <xs:enumeration value="PTC-CJA_N"/>
         <xs:enumeration value="PTC-VIKS"/>
         <xs:enumeration value="PTC-AN"/>
         <xs:enumeration value="PTC-ANA"/>
         <xs:enumeration value="PTC-OM"/>
         <xs:enumeration value="PTC-TOM"/>
         <xs:enumeration value="PTC-YSJ"/>
         <xs:enumeration value="PTC_N"/>
         <xs:enumeration value="REFL"/>
         <!-- spatial This will be further divided into DEICT, INDEP, DEPEND, INTERR -->
         <xs:enumeration value="RECIP">
            <!--Reciprocal-->
         </xs:enumeration>
         <xs:enumeration value="SPAT"/>
         <xs:enumeration value="SPAT-INDEF"/>
         <xs:enumeration value="SPAT-DEM"/>
         <xs:enumeration value="SPAT-DISTR"/>
         <xs:enumeration value="ADV-SPAT-GEN"/>
         <xs:enumeration value="SPAT-DEICT"/>
         <xs:enumeration value="SPAT-INDEP"/>
         <xs:enumeration value="SPAT-DEPEND"/>
         <xs:enumeration value="SPAT-GOAL"/>
         <xs:enumeration value="SPAT-SOURCE"/>
         <xs:enumeration value="SPAT-LOC"/>
         <xs:enumeration value="SPAT-MULTIPOINT"/>
         <xs:enumeration value="STATE"/>
         <xs:enumeration value="SUMMON"/>
         <xs:enumeration value="TEMP"/>
         <xs:enumeration value="TEMP-SPAT"/>
         <xs:enumeration value="TEMP-DISTR"/>
         <xs:enumeration value="TERMINATIVE">
            <!--Adverb indicating temporal terminative limit. Erzya: -->
         </xs:enumeration>
         <xs:enumeration value="QNT"/>
         <xs:enumeration value="QNT-ADN"/>
         <xs:enumeration value="QNT-ADV"/>
         <xs:enumeration value="QNT-PRON"/>
         <xs:enumeration value="QNT-INDEF"/>
         <xs:enumeration value="QNT-COLL"/>
         <xs:enumeration value="QNT-COLL-UNIV"/>
         <xs:enumeration value="QNT-DISTR"/>
         <xs:enumeration value="QNT-GEN"/>
         <xs:enumeration value="QNT-ITER"/>
         <xs:enumeration value="QNT-UNIV"/>
         <xs:enumeration value="QNT-UNIV-ADN"/>
         <xs:enumeration value="QNT-UNIV-PRON"/>
         <xs:enumeration value="V"/>
         <xs:enumeration value="V-V"/>
         <xs:enumeration value="VT"/>
         <xs:enumeration value="VI"/>
         <xs:enumeration value="V-AUX"/>
         <xs:enumeration value="V-FINITE"/>
         <xs:enumeration value="V-NEG"/>
         <xs:enumeration value="VT-IMP"/>
         <xs:enumeration value="VI-IMP"/>
         <xs:enumeration value="V-IMP-PRT"/>
         <xs:enumeration value="V-REFL"/>
         <xs:enumeration value="V-INST"/>
         <xs:enumeration value="PRON-INTERR"/>
         <xs:enumeration value="Q_A"/>
         <xs:enumeration value="Q_ADV"/>
         <xs:enumeration value="Q_DET"/>
         <xs:enumeration value="Q_ITER"/>
         <xs:enumeration value="Q_MANNER"/>
         <xs:enumeration value="Q_N"/>
         <xs:enumeration value="Q_ORD"/>
         <xs:enumeration value="Q_ORD-ITER"/>
         <xs:enumeration value="Q_PCLE"/>
         <xs:enumeration value="Q_PRON"/>
         <xs:enumeration value="Q_PERS"/>
         <xs:enumeration value="Q_QNT"/>
         <xs:enumeration value="Q_REASON"/>
         <xs:enumeration value="Q_SPAT"/>
         <xs:enumeration value="Q_TEMP"/>
         <xs:enumeration value="R_ORD"/>
         <xs:enumeration value="R_QNT"/>
         <xs:enumeration value="R_SPAT"/>
         <xs:enumeration value="R_TEMP"/>
         <xs:enumeration value="PRT_INTERR"/>
         <xs:enumeration value="R_PRON"/>
         <xs:enumeration value="VB"/>
         <xs:enumeration value="VB1"/>
         <xs:enumeration value="VB2"/>
         <xs:enumeration value="DESCR"/>
         <xs:enumeration value="MWE"/>
         <xs:enumeration value="PCLE"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="src">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="kpvSrc"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:attribute name="orthType">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="allegro"/>
            <xs:enumeration value="dial"/>
            <xs:enumeration value="erz"/>
            <xs:enumeration value="Kozl"/>
            <xs:enumeration value="morph"/>
            <xs:enumeration value="old"/>
            <xs:enumeration value="phon"/>
            <xs:enumeration value="rus"/>
            <xs:enumeration value="ry"/>
            <xs:enumeration value="uncertain"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:element name="phonet">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string"/>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:attribute name="Contlex" type="xs:string"/>


   <xs:element name="drv">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="cx"/>
               <xs:attribute ref="drvType"/>
               <xs:attribute ref="dx"/>
               <xs:attribute ref="lemmaID"/>
               <xs:attribute ref="stemID"/>
               <xs:attribute ref="kpvMorph"/>
               <xs:attribute ref="myvMorph"/>
               <xs:attribute ref="mx"/>
               <xs:attribute ref="nx"/>
               <xs:attribute ref="px"/>
               <xs:attribute ref="tx"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="morphg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="morph"/>
            <xs:element ref="morphCtn"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="morph">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="constituent" type="xs:string"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="t"/>
         </xs:sequence>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="A"/>
                  <xs:enumeration value="N"/>
                  <xs:enumeration value="PROP"/>
                  <xs:enumeration value="V"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="morphCtn">
      <xs:complexType>
         <xs:choice>
            <xs:element maxOccurs="unbounded" minOccurs="2" ref="morph"/>
         </xs:choice>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="stemVariation"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="etymg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="etym"/>
            <xs:element ref="etymCtn"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="etymCtn">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="2" ref="etym"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="t"/>
         </xs:sequence>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="stemVariation"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="etym">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="constituent" type="xs:string"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="t"/>
         </xs:sequence>
         <xs:attribute ref="cx"/>
         <xs:attribute ref="dx"/>
         <xs:attribute name="langForm">
            <!--This should be either dial or *-->
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="dial"/>
                  <xs:enumeration value="*"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute ref="etymType"/>
         <xs:attribute ref="lang"/>
         <xs:attribute ref="lemmaID"/>
	 <xs:attribute ref="stemID"/>
         <xs:attribute ref="nx"/>
      </xs:complexType>
   </xs:element>
   <xs:attribute name="drvType">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="Allegro"/>
            <xs:enumeration value="Descr">
<!-- изобр. descriptive-->	    </xs:enumeration>
            <xs:enumeration value="Dim"/>
            <xs:enumeration value="Dim/не"/>
            <xs:enumeration value="Dim/ке"/>
            <xs:enumeration value="Distr"/>
            <xs:enumeration value="DV_N"/>
            <xs:enumeration value="Der/а"/>
            <xs:enumeration value="Der/ана"/>
            <xs:enumeration value="Der/ан"/>
            <xs:enumeration value="Der/Ин"/>
            <xs:enumeration value="Der/ӧм"/>
            <xs:enumeration value="Der/тӧм"/>
            <xs:enumeration value="Der/ысь"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="lemmaID" type="xs:string"/>
   <xs:attribute name="stemID" type="xs:string"/>
   <xs:element name="tg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="t"/>
            <xs:element ref="tCtn"/>
         </xs:choice>
         <xs:attribute ref="lang"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="tCtn">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="t"/>
