<!-- присоединит.-->
	 </xs:enumeration>
         <xs:enumeration value="CC-EMPHATIC">
<!--усил., выделит. -->
	 </xs:enumeration>
         <xs:enumeration value="CS">
            <!--SUBORD "subordinator"-->
         </xs:enumeration>
