         <xs:enumeration value="INE_N"/>
         <xs:enumeration value="NN"/>
         <xs:enumeration value="N-Assoc"/>
         <xs:enumeration value="N-Coll"/>
         <xs:enumeration value="N-Pl"/>
         <xs:enumeration value="N-Sg"/>
         <xs:enumeration value="N-Temp">
