<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty">
   <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
<!--<xs:include schemaLocation="semanticSetFor-dict-schema.xsd"/> This has been brought back into the schema 2013-11-11 -->
<xs:include schemaLocation="kpv-dict-schema.xsd"/>
<xs:include schemaLocation="mrj-dict-schema.xsd"/>
<xs:include schemaLocation="myv-dict-schema.xsd"/>
   <xs:element name="r">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="lics"/>
            <xs:element maxOccurs="unbounded" ref="e"/>
         </xs:sequence>
         <xs:attribute ref="xml:lang"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="lics">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="lic"/>
            <xs:element minOccurs="0" ref="ref"/>
            <xs:element minOccurs="0" ref="sourcenote"/>
         </xs:sequence>
         <xs:attribute ref="xml:space"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="lic">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element ref="a"/>
         </xs:sequence>
         <xs:attribute ref="xml:lang"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="ref">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element ref="a"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="sourcenote">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element ref="a"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="a">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string"/>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="e">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="map"/>
            <xs:element minOccurs="0" name="rev-sort_key" type="xs:string"/>
            <xs:element ref="lg"/>
            <xs:element minOccurs="0" ref="posDescr"/>
            <xs:element minOccurs="0" ref="semRole"/>
            <xs:element minOccurs="0" ref="phonet"/>
            <xs:element minOccurs="0" ref="morphg"/>
	   <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element minOccurs="0" ref="drv"/>
            <xs:element minOccurs="0" ref="etymg"/>
	   </xs:choice>
            <xs:element minOccurs="0" ref="sources"/>
            <xs:element maxOccurs="unbounded" ref="mg"/>
            <xs:element minOccurs="0" ref="content"/>
         </xs:sequence>
         <xs:attribute ref="exclude"/>
         <xs:attribute name="r" type="xs:string"/>
         <xs:attribute name="order">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="1"/>
                  <xs:enumeration value="2"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
	 <xs:attribute name="id" type="xs:string"/>
	 <xs:attribute name="meta" type="xs:string"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="map">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute name="fin_lemma_id" type="xs:string"/>
               <xs:attribute name="stamp" type="xs:string"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="lg">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="l"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_ref"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_drv"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_cogn"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="lsub"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="variants"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_sci"/>
            <xs:element ref="stg"/>
            <xs:element minOccurs="0" ref="audio"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="inc-audio"/>
            <xs:element minOccurs="0" ref="mini_paradigm"/>
            <xs:element minOccurs="0" ref="etymology"/>
            <xs:element minOccurs="0" ref="compg"/>
            <xs:element ref="stress" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="variants">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_syn"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="l_var"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="stg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="st"/>
            <xs:element ref="mini_paradigm"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="l">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="gen"/>
               <xs:attribute ref="hid"/>
               <xs:attribute ref="pos" use="required"/>
               <xs:attribute ref="src"/>
               <xs:attribute name="type" type="PosType"/>
               <xs:attribute ref="val"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_ref">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
	      <xs:attribute ref="pos" use="required"/>
               <xs:attribute name="type">
		 <xs:simpleType>
		   <xs:restriction base="xs:string">
		   </xs:restriction>
		 </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_drv"><!-- This is used with derivations. The content ends in _POS , but there is also a pos attribute-->
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
	      <xs:attribute ref="pos" use="required"/>
               <xs:attribute name="type">
		 <xs:simpleType>
		   <xs:restriction base="xs:string">
		   </xs:restriction>
		 </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_cogn">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
	      <xs:attribute ref="pos" use="required"/>
               <xs:attribute name="type">
		 <xs:simpleType>
		   <xs:restriction base="xs:string">
		   </xs:restriction>
		 </xs:simpleType>
               </xs:attribute>
               <xs:attribute ref="src"/>
               <xs:attribute ref="xml:lang" use="required"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
<xs:element name="semRole" type="xs:string"/>
   <xs:element name="lsub">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="myvSrc"/>
               <xs:attribute ref="orthType"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_syn">
      <xs:complexType mixed="true">
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="pl"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_var">
      <xs:complexType mixed="true">
         <xs:simpleContent>
            <xs:extension base="xs:string">
         <xs:attribute ref="gloss"/>
               <xs:attribute ref="pl"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="l_sci">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="src"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="st">
      <xs:complexType mixed="true">
         <xs:sequence/>
         <xs:attribute ref="exclude"/>
         <xs:attribute ref="Contlex"/>
         <xs:attribute ref="inflexId"/>
         <xs:attribute name="NumContlex" type="xs:string"/>
         <xs:attribute ref="orthType"/>
         <xs:attribute ref="status"/>
	 <xs:attribute name="varid" type="xs:string"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="mini_paradigm">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="analysis"/>
         </xs:sequence>
         <xs:attribute ref="exclude"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="stress">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:attribute name="pl" type="xs:string"/>
   <xs:simpleType name="PosType">
     <xs:restriction base="xs:string">
       <xs:enumeration value="Card"/>
       <xs:enumeration value="Coll"/>
       <xs:enumeration value="Dem"/>
       <xs:enumeration value="Distr"/>
       <xs:enumeration value="Indef"/>
       <xs:enumeration value="Interr"/>
       <xs:enumeration value="Iter"/>
       <xs:enumeration value="Ord"/>
       <xs:enumeration value="Pers"/>
       <xs:enumeration value="Prop"/>
       <xs:enumeration value="Recip"/>
       <xs:enumeration value="Refl"/>
       <xs:enumeration value="Rel"/>
     </xs:restriction>
   </xs:simpleType>

   <xs:element name="pos" type="posCategory"/>
   <xs:attribute name="pos" type="posCategory"/>
   <xs:simpleType name="posCategory">
      <xs:restriction base="xs:string">
         <xs:enumeration value="A"/>
         <xs:enumeration value="Ad-A"/>
         <xs:enumeration value="Adv"/>
         <xs:enumeration value="Art"/>
         <xs:enumeration value="Clt"/>
         <xs:enumeration value="Conj">
            <!--Conjunction that still needs to be classified as either coordinator or subordinator-->
         </xs:enumeration>
         <xs:enumeration value="CC">
            <!--COORD "coordinator"-->
         </xs:enumeration>
         <xs:enumeration value="CS">
            <!--SUBORD "subordinator"-->
         </xs:enumeration>
         <xs:enumeration value="Descr"/>
         <xs:enumeration value="Det"/>
         <xs:enumeration value="Ger"/>
         <xs:enumeration value="Interj"/>
         <xs:enumeration value="Ideoph"/>
         <xs:enumeration value="N"/>
         <xs:enumeration value="NN"/>
         <xs:enumeration value="Num"/>
         <xs:enumeration value="Pcle"/>
         <xs:enumeration value="Suf"/>
         <xs:enumeration value="Prefix"/>
         <xs:enumeration value="Po"/>
         <xs:enumeration value="Pr"/>
         <xs:enumeration value="Prc"/>
         <xs:enumeration value="Pred2">
            <!--secondary predication: чавонь кедть, ласте, озадо-->
         </xs:enumeration>
         <xs:enumeration value="Pron"/>

         <xs:enumeration value="Qnt"/>
         <xs:enumeration value="V"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="posDescr" type="posDescrType"/>
   <xs:attribute name="posDescr" type="posDescrType"/>
   <xs:simpleType name="posDescrType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="SUF"/>
         <xs:enumeration value="PREF"/>
         <xs:enumeration value="Phrase"/>
         <xs:enumeration value="PHRASE_A"/>
         <xs:enumeration value="PHRASE_ADV"/>
