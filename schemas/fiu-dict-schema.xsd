         </xs:enumeration>
         <xs:enumeration value="N-Vocative">
<!-- depricating to VOCATIVE -->
	 </xs:enumeration>
         <xs:enumeration value="Vocative"/>
         <xs:enumeration value="Num"/>
         <xs:enumeration value="NUM-ADN"/>
         <xs:enumeration value="NUM-UNIV"/>
         <xs:enumeration value="NUM-DISTR">
<!-- кыкӧн-кыкӧн -->
